{
  "name": "tmdt-backend",
  "version": "1.0.0",
  "description": "Backend codebase using NodeJS + Express + PostgreSQL",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "build": "rimraf ./dist && npx tsc",
    "dev": "nodemon ./src/server.ts",
    "start": "npm run build && node -r ts-node/register ./dist/server.js",
    "staging": "npm run build && NODE_ENV=staging node -r ts-node/register ./dist/server.js",
    "production": "npm run build && NODE_ENV=production node -r ts-node/register ./dist/server.js",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:up": "npx sequelize-cli db:seed --seed",
    "seed:down": "npx sequelize-cli db:seed:undo --seed",
    "seed:generate": "npx sequelize-cli seed:generate --name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hungpd90/tmdt-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hungpd90/tmdt-backend/issues"
  },
  "homepage": "https://github.com/hungpd90/tmdt-backend#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^4.2.0",
    "@types/express": "^4.17.16",
    "@types/morgan": "^1.9.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "numjs": "^0.16.1",
    "scikitjs": "^1.24.0",
    "sequelize": "^6.29.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13"
  }
}
